###  :SupervisedApproach

:SupervisedApproach a owl:Class ;
                                                                            
                                                                            owl:equivalentClass [ a owl:Class ;
       owl:intersectionOf ( :Algorithm
                                                                                                                 [ a owl:Restriction ;
                           owl:onProperty :hasLearningMethod ;
                           owl:onClass :Supervised ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                                                                 ]
                                                                                                               )
                                                                                             ] ,
                                                                                             [ a owl:Class ;
       owl:intersectionOf ( :Algorithm
                                                                                                                 [ a owl:Class ;
                           owl:unionOf ( [ a owl:Restriction ;
                                        owl:onProperty :hasLearningProblem ;
                                        owl:onClass :ClassificationProblem;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                                                                              ]
                                                                                                                              [ a owl:Restriction ;
                                        owl:onProperty :hasLearningProblem ;
                                        owl:onClass :Regression ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                                                                                              ]
                                                                                                                            )
