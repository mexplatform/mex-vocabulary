  a owl:Ontology ;  
  owl:versionInfo "1.0" ;
  rdfs:label "MEX-Algorithm"@en ;
  rdfs:comment "MEX Algorithm (mex-algo) is an ontology with the goal of describing machine learning algorithms"@en ;
  dct:created "2014-02-02"^^xsd:date ;
  dct:modified "2014-04-05"^^xsd:date ;
  dc:publisher "AKSW Research Group - Leipzig University" ;
  dc:creator "Diego Esteves" ;
  dc:contributor "Jens Lehmann", "Julio Cesar Duarte", "Markus Ackermann" ;
  dct:title "MEX Algorithm Ontology (mex-algo)"@en ;
  owl:imports doap:, rdfs:, prov: .


#################################################################
#    Object Properties
#################################################################

###  mex-algo#hasAlgorithmConfiguration

:hasAlgorithmConfiguration 
  a owl:ObjectProperty ,
  owl:TransitiveProperty 
.



###  mex-algo#isAlgorithmConfigurationOf

:isAlgorithmConfigurationOf 
  a owl:ObjectProperty ,
  owl:TransitiveProperty 
.



###  mex-algo#isAlgorithmParameterCollectionOf

:isAlgorithmParameterCollectionOf 
  a owl:FunctionalProperty ,
  owl:InverseFunctionalProperty ,
  owl:ObjectProperty ;
  rdfs:range :Algorithm ;
  rdfs:subPropertyOf :isAlgorithmConfigurationOf ;
  rdfs:domain :AlgorithmParameter ;
  owl:inverseOf :hasAlgorithmParameterCollection 
.



###  mex-algo#isClassOf

:isClassOf a owl:IrreflexiveProperty ,
  owl:ObjectProperty ; 
  rdfs:range :Algorithm ; 
  rdfs:subPropertyOf :isAlgorithmConfigurationOf ; 
  rdfs:domain :AlgorithmClass ; 
  owl:inverseOf :hasClass 
.

###  mex-algo#isImplementationOf

:isImplementationOf a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ; rdfs:range :Algorithm ;  rdfs:subPropertyOf :isAlgorithmConfigurationOf ;  rdfs:domain :Implementation ; owl:inverseOf :hasImplementation .


###  mex-algo#isLearningMethodOf

:isLearningMethodOf a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ; rdfs:range :Algorithm ;  rdfs:subPropertyOf :isAlgorithmConfigurationOf ;  rdfs:domain :LearningMethod ; owl:inverseOf :hasLearningMethod .


###  mex-algo#isLearningProblemOf

:isLearningProblemOf a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ; rdfs:range :Algorithm ;  rdfs:subPropertyOf :isAlgorithmConfigurationOf ;  rdfs:domain :LearningProblem ; owl:inverseOf :hasLearningProblem .


###  http://www.w3.org/2002/07/owl#hasAlgorithmParameterCollection

:hasAlgorithmParameterCollection a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:IrreflexiveProperty , owl:ObjectProperty ; rdfs:domain :Algorithm ;  rdfs:range :AlgorithmParameterCollection ;  rdfs:subPropertyOf :hasAlgorithmConfiguration .


###  http://www.w3.org/2002/07/owl#hasClass

:hasClass a owl:IrreflexiveProperty , owl:ObjectProperty ; rdfs:domain :Algorithm ;  rdfs:subPropertyOf :hasAlgorithmConfiguration ;  rdfs:range :AlgorithmClass .


###  http://www.w3.org/2002/07/owl#hasImplementation

:hasImplementation a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:ObjectProperty ; rdfs:domain :Algorithm ;  rdfs:subPropertyOf :hasAlgorithmConfiguration ;  rdfs:range :Implementation .


###  http://www.w3.org/2002/07/owl#hasLearningMethod

:hasLearningMethod a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:IrreflexiveProperty , owl:ObjectProperty ; rdfs:domain :Algorithm ;  rdfs:subPropertyOf :hasAlgorithmConfiguration ;  rdfs:range :LearningMethod .


###  http://www.w3.org/2002/07/owl#hasLearningProblem

:hasLearningProblem a owl:FunctionalProperty , owl:InverseFunctionalProperty , owl:IrreflexiveProperty , owl:ObjectProperty ; rdfs:domain :Algorithm ;  rdfs:subPropertyOf :hasAlgorithmConfiguration ;  rdfs:range :LearningProblem .

prov:wasDerivedFrom
    a owl:ObjectProperty ;
    rdfs:comment "Entity the experiment was derived from"@en ;
    rdfs:domain prov:Entity ;
    rdfs:label "was derived from"@en ;
    rdfs:range prov:Entity .

prov:hadMember
    a owl:ObjectProperty ;
    rdfs:domain prov:Collection ;
    rdfs:label "had member"@en ;
    rdfs:range prov:Entity .


#################################################################
#    Data properties
#################################################################


###  http://www.w3.org/2002/07/owl#acronym

:acronym a owl:DatatypeProperty ; rdfs:range xsd:string ; rdfs:domain :NamedAlgorithm .




#################################################################
#    Classes
#################################################################


###  mex-algo#ADTree

:ADTree a owl:Class ; rdfs:subClassOf :NamedAlgorithm , [ a owl:Restriction ; owl:onProperty :hasClass ; owl:someValuesFrom :DecisionTrees ] .


###  mex-algo#AQ

:AQ a owl:Class ; rdfs:subClassOf :NamedAlgorithm , [ a owl:Restriction ; owl:onProperty :hasClass ; owl:onClass owl:Rules ;  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .


###  mex-algo#AdaptativeBoost

:AdaptativeBoost a owl:Class ; rdfs:label "AdaBoost"@en ; rdfs:subClassOf :NamedAlgorithm , [ a owl:Restriction ; owl:onProperty :hasClass ;  owl:onClass :Boosting ;  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .


###  mex-algo#Algorithm

:Algorithm a owl:Class ; 
  rdfs:subClassOf prov:Entity, [ 
      a owl:Restriction ; 
      owl:onProperty :hasLearningMethod ; 
      owl:someValuesFrom :LearningMethod 
    ] , [ 
      a owl:Restriction ; 
      owl:onProperty :hasClass ; 
      owl:someValuesFrom :AlgorithmClass 
    ] , [ 
      a owl:Restriction ; 
      owl:onProperty :hasLearningProblem ; 
      owl:someValuesFrom :LearningProblem 
    ] .


###  mex-algo#AlgorithmParameterCollection

:AlgorithmParameterCollection a owl:Class ; 
  rdfs:subClassOf prov:Collection .



